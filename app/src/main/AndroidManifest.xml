        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.VictoryVault"
        tools:targetApi="31">
        <activity
            android:name="com.victoryvault.java.edit_profile"
